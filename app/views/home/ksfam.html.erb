<p id="notice"><%= notice %></p>

<h1>Employees</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Position</th>
      <th>Phone Number</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

   
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %> <%=user.last_name %></td>
        <td><%= user.position %></td>
        <td><%= user.phone_number %></td>
        <td><%= user.email %></td>   
      </tr>
    <% end %>
  </tbody>
</table>

<br>
